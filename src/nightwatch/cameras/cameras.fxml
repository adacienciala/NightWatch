<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>

<StackPane onScroll="#zoomCam" prefHeight="400.0" prefWidth="600.0" stylesheets="@cameras_style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nightwatch.cameras.CamerasController">
   <children>
      <Pane fx:id="screen" style="-fx-background-color: #4D4D4D;" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </StackPane.margin>
      </Pane>
      <Pane prefHeight="200.0" prefWidth="200.0" styleClass="camera-frame" />
      <Label fx:id="detailsLabel" text="Label" StackPane.alignment="TOP_LEFT">
         <StackPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </StackPane.margin>
      </Label>
   </children>
</StackPane>
